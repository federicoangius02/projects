# Variabili AWS (usa valori fittizi - non collegarti ad AWS reale in sviluppo)
AWS_REGION=eu-south-1
S3_BUCKET_NAME=my-dev-bucket-federico
DYNAMODB_TABLE_NAME=my-dev-table-federico

# Configurazione server
PORT=3000
NODE_ENV=development

# Credenziali fittizie (non usare quelle reali!)
AWS_ACCESS_KEY_ID=fake_access_key
AWS_SECRET_ACCESS_KEY=fake_secret_key

# Opzionale: abilita logging dettagliato
DEBUG=true